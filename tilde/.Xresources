! vim:ft=xdefaults:

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

XTerm.VT100.termName: xterm-256color
XTerm.VT100.internalBorder: 140
XTerm.VT100.geometry: 52x12
XTerm.VT100.scaleHeight: 1.0
XTerm.VT100.cursorBlink: false
XTerm.VT100.scrollBar: false
XTerm.VT100.selectToClipboard: true
XTerm.VT100.utf8: true
XTerm.VT100.renderFont: true
XTerm.VT100.metaSendsEscape: true
XTerm.VT100.faceName: xft:SF Mono:pixelsize=35:antialias=true
XTerm.VT100.faceNameDoublesize: xft:SF Mono:pixelsize=35:antialias=true
XTerm.VT100.wideChars: true
XTerm.VT100.wideFont: xft:SF Mono:pixelsize=35:antialias=true

! XTerm.VT100.translations: #override\nShift Ctrl <KeyPress> v:insert-selection(CLIPBOARD)\nShift Ctrl <KeyPress> c:select-set(CLIPBOARD)

*background: #232323
*foreground: #FCFCFC
*cursorColor: #FCFCFC

*color0:  #232323
*color1:  #C7AA9C
*color2:  #C7B9AC
*color3:  #D4BAAD
*color4:  #C6BABA
*color5:  #D4BCBC
*color6:  #B9B9B9
*color7:  #FCFCFC
*color8:  #808080
*color9:  #C7AA9C
*color10: #C7B9AC
*color11: #D4BAAD
*color12: #C6BABA
*color13: #D4BCBC
*color14: #B9B9B9
*color15: #FCFCFC
