! vim:ft=xdefaults:

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

xterm*termName: xterm-256color
xterm*scaleHeight: 1.0
xterm*geometry: 52x12
xterm*internalBorder: 140
xterm*borderWidth: 0

xterm*utf8: true
xterm*scrollBar: false
xterm*cursorBlink: false
xterm*selectToClipboard: true
xterm*trimSelection: true
xterm*renderFont: true
xterm*wideChars: true
xterm*visualBell: false

xterm.vt100.translations: #override \n\
    Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
    Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

xterm*faceName: xft:SF Mono:pixelsize=35:antialias=true
xterm*faceNameDoublesize: xft:SF Mono:pixelsize=35:antialias=true
xterm*wideFont: xft:SF Mono:pixelsize=35:antialias=true

*background: #272822
*foreground: #f8f8f2
*cursorColor: #bfc0ba

*color0:  #272822
*color8:  #7f807a

*color1:  #c6442c
*color9:  #c6442c

*color2:  #93c62c
*color10: #93c62c

*color3:  #f8f8f2
*color11: #f8f8f2

*color4:  #9671d9
*color12: #9671d9

*color5:  #f82672
*color13: #f82672

*color6:  #65d6eb
*color14: #65d6eb

*color7:  #f8f8f2
*color15: #f8f8f2
